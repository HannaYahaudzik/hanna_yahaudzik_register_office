<?xml version="1.0"?>

<!DOCTYPE suppressions PUBLIC
        "-//Checkstyle//DTD SuppressionFilter Configuration 1.0//EN"
        "https://checkstyle.org/dtds/suppressions_1_0.dtd">

<suppressions>

    <!-- Doesn't check that a package-info.java file exists for each package. -->
    <!-- See http://checkstyle.sf.net/config_javadoc.html#JavadocPackage      -->
    <suppress files="" checks="JavadocPackage"/>

    <!-- Doesn't check for Naming Conventions.                  -->
    <!-- See http://checkstyle.sf.net/config_naming.html -->
    <suppress checks="MemberName" files="" />

    <!-- Checks for Javadoc comments.                     -->
    <!-- See http://checkstyle.sf.net/config_javadoc.html -->
    <suppress checks="JavadocVariable" files=""/>

    <!-- Doesn't check for Javadoc comments.                     -->
    <!-- See http://checkstyle.sf.net/config_javadoc.html -->
    <suppress files="" checks="JavadocVariable"/>

    <!-- Doesn't check for common coding problems               -->
    <!-- See http://checkstyle.sf.net/config_coding.html -->
    <suppress files="entity" checks="HiddenField"/>

</suppressions>
